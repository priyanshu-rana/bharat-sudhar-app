{
  "cli": {
    "version": ">= 0.34.0",
    "requireCommit": false,
    "appVersionSource": "remote"
  },
  "build": {
    "preview": {
      "android": {
        "buildType": "apk",
        "ndk": "26.1.10909125",
        "gradleCommand": "clean :app:assembleDebug"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://example.com"
      }
    },
    "production": {
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "autoIncrement": true,
        "distribution": "store",
        "ndk": "26.1.10909125"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://example.com"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
